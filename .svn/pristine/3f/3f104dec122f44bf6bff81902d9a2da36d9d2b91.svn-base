  data() {
    return {
      formName:"customerFm",
      currentSubItemlength_addr:0, // 当前子表数据个数
      currentSubItemlength_contact:0, // 当前子表数据个数
      requestBaseUrl: "/sale/customer", // 请求 查询 操作的基础路径
      formDataInfo:Object.assign({},default_formDataInfo),// 防止添加和更新数据提交发生冲突
