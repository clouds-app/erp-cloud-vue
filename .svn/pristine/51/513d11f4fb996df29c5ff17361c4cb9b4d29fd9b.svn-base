      this.currentSubItemlength_productMData=0
      //判断当前子表 添加/更新的 数据个数
      if(this.action==='add'){
          if(tableData.addList.length>0){
            this.currentSubItemlength_productMData=Object.keys(tableData.addList[0]).length
          }
      }else{
          if(tableData.deleteList.length>0){
                this.currentSubItemlength_productMData= Object.keys(tableData.deleteList[0]).length
          }
          if(tableData.updateList.length>0){
                this.currentSubItemlength_productMData= Object.keys(tableData.updateList[0]).length+this.currentSubItemlength_productMData
          }
          if(tableData.addList.length>0){
            this.currentSubItemlength_productMData=Object.keys(tableData.addList[0]).length+this.currentSubItemlength_productMData
          }
           
      }
      return this.currentSubItemlength_productMData

    },


