      table: [], //存储子表全部对应的字段
      title: "客户编号"
      //propvalue:'custCode'//存储子表每个对应的字段
    };
  },
  computed: {
    getppoQty() {
      //debugger
      let tableData = this.$refs["slave_edit-purPaperPo"].cloneData;
      return (tableData[0].ppoQty =
        tableData[0].ppoStockQty -
        tableData[0].ppoInQty +
        tableData[0].ppoRQty);
