    clickmaster() {
      //debugger
      let ddata = this.formDataInfo;
      // if (!!ddata.master.beginDate) {
      //   ddata.master.beginDate = dayjs().format(
      //     "YYYY-MM-DD HH:mm:ss"
      //   );
      // }
      // if (!!ddata.master.endDate) {
      //   ddata.master.endDate = dayjs().format(
      //     "YYYY-MM-DD HH:mm:ss"
      //   );
      // }
      // let suplierid = this.List
      let one = {
        // inCompanyId:"",
        ppoGroupNo: this.formDataInfo.master.ppoGroupNo,
        // ppoNo:"",
        // inProvider:this.formDataInfo.master.supplierId,
        beginDate: this.formDataInfo.master.beginDate,
        endDate: this.formDataInfo.master.endDate,
        flag: this.formDataInfo.master.flag,
        ppoGroupNoList:this.ppoGroupNoList
      };
      var _this = this;
      request
        .post(`/stock/paperJoin/getSpPaperPOToPaperJoin`, one)
        .then(res => {
          if (res) {
            for (let i = 0; i < res.length; i++) {
              if(res[i].ppoDate)
              res[i].ppoDate = res[i].ppoDate.replace("T",' ').replace('.000+0000',"")
              if(res[i].ppoDueDate)
              res[i].ppoDueDate = res[i].ppoDueDate.replace("T",' ').replace('.000+0000',"") 
              // res[i].ppoDate = dayjs().format("YYYY-MM-DD HH:mm:ss"); //ppoDate
              // res[i].ppoDueDate = dayjs().format(
              //   "YYYY-MM-DD HH:mm:ss"
              // ); 
            }
          }
          //debugger
          _this.$refs["slave_edit-purPaperPo"].cloneData = res;
        });
    },

    //加载表单初始化数据
    getFormInitDataObj(data) {
      //debugger
      //加载表单初始化数据
      this.formDataInfo["master"] = {
        inCusCode: "",
        propvalue: "",
        inCusName: "",
        flag: "0"
        // beginDate:dayjs().format("2000-11-30"),
        // endDate:dayjs().format("YYYY-MM-DD"),
        // ifAreaPrice:false,
      };
      if (this.$refs["slave_edit-purPaperPo"])
        this.$refs["slave_edit-purPaperPo"].cloneData = data;
    },

    //表单数据提交事件
    submitFormDataEvent() {
      //debugger
      //表单数据提交事件
      // true就是有问题
      let result = this.$refs["slave_edit-purPaperPo"].validate();
      if (result) {
        return;
      }
      let parms = [];
      let submitDataObj = this.$refs["slave_edit-purPaperPo"].cloneData;
      for (let index = 0; index < submitDataObj.length; index++) {
        let Choice = this.$refs["slave_edit-purPaperPo"].cloneData[index]
          .Choice;
        if (Choice === true) {
          parms.push(submitDataObj[index]);
        }
      }
      //提交数据
      // 向外暴露的方法:
      this.$emit("closeMain", parms);
      this.showWindow = false; // 关闭当前窗口
      // });
    },
      // 重写父类 关闭窗口时 触发事件
    closeActionTigger () {
      ////debugger
      // fix 清除上次的错误提示 formDataInfo 为表单ref名称
      if(this.$refs["masterForm"])
      this.$refs["masterForm"].resetFields();
      if(this.$refs["slave_edit-purPaperPo"])
      this.$refs["slave_edit-purPaperPo"].reset();
    },
  },
  created() {
    this.getFormInitDataObj(); //获取初始化数据
  }
