        stationId:this.whId,
        // issInput: 0,
        likeFlag: this.formDataInfo.master.likeFlag
      };
      request.post(`/stock/boxLibLost/getBoxStoreData`, params).then(res => {
        this.$refs["slave_edit-boxUseLost"].cloneData = res;
      });
    },

    //加载表单初始化数据
