        this.workNo = "";
      } else if (this.propvalue == "cusName") {
        this.cusName = this.formDataInfo.master.inBiWorkNo;
        this.biProdName = "";
        this.workNo = "";
      } else {
        this.workNo = this.formDataInfo.master.inBiWorkNo;
